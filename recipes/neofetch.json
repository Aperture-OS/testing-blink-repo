{
  "name": "neofetch",
  "version": "7.1.0",
  "release": 1765660122,
  "description": "Neofetch is a command-line system information tool written in bash 3.2+.",
  "author": "github.com/dylanaraps/neofetch",
  "license": "MIT",
  "source": {
    "url": "https://github.com/dylanaraps/neofetch/archive/refs/tags/7.1.0.tar.gz",
    "sha256": "58a95e6b714e41efc804eca389a223309169b2def35e57fa934482a6b47c27e7",
    "type": "tar.gz"
  },
  "dependencies": {
    "null": ">=1.0.0",
	"null1": ">=1.0.0"
  },
  "opt_dependencies": [
    {
      "id": 1,
      "description": "Choose one or more image rendering backend. This enables image display in supported terminals.",
      "options": [
        "null1",
        "circular",
        "w3m-img",
        "iTerm2",
        "Terminology",
        "kitty",
        "libsixel",
        "termpix",
        "pixterm",
        "catimg",
        "jp2a",
        "caca-utils",
        "libcaca"
      ]
    }
  ],

  "build": {
    "kind": "toCompile",
    "env": {
      "MAKEFLAGS": "-j$(nproc)"
    },
    "prepare": ["echo 'Preparing build environment...'"],
    "install": ["make install PREFIX=${PREFIX:-/usr/local}"],
    "uninstall": ["make uninstall PREFIX=${PREFIX:-/usr/local}"]
  }
}
