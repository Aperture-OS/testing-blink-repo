{
    "name": "null",
    "version": "1.0.0",
    "release": 1766873182,
    "description": "Null is a test recipe that makes a file, its purpose is none :D",
    "author": "github.com/Aperture-OS/blink-package-manager",
    "license": "The Unlicense",
    "source": {
        "url": "https://github.com/dylanaraps/neofetch/archive/refs/tags/7.1.0.tar.gz",
        "sha256": "58a95e6b714e41efc804eca389a223309169b2def35e57fa934482a6b47c27e7",
        "type": "tar.gz"
    },
    "dependencies": {
        "bash": ">=3.2"
    },
    "opt_dependencies": [
        {
            "id": 1,
            "description": "Choose one or more image rendering backend. This enables image display in supported terminals.",
            "options": [
                "w3m-img",
                "iTerm2",
                "Terminology",
                "kitty",
                "libsixel",
                "termpix",
                "pixterm",
                "catimg",
                "jp2a",
                "caca-utils",
                "libcaca"
            ],
            "default": "w3m-img"
        }
    ],
    "build": {
        "env": {
            "exampleVariable12345="hi apertureos""
        },
        "prepare": [
            "echo 'Preparing build environment...'"
        ],
        "install": [
            "touch /home/Desktop/ApertureOS/blink/example.null"
        ],
        "uninstall": [
            "touch /home/Desktop/ApertureOS/blink/example.null"
        ]
    }
}
